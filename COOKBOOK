#### MySQL

CREATE USER 'skrik'@'localhost' IDENTIFIED BY 'skrik123';
CREATE DATABASE skrik;
GRANT DELETE, INSERT, SELECT, UPDATE ON skrik . * TO 'skrik'@'localhost';

USE skrik;
CREATE TABLE user ( id BIGINT NOT NULL AUTO_INCREMENT PRIMARY KEY,
         name VARCHAR(50),
         email VARCHAR(100),
         status VARCHAR(250)
       );
CREATE TABLE msging ( id BIGINT NOT NULL AUTO_INCREMENT PRIMARY KEY,
         userid_from BIGINT NOT NULL,
         userid_to BIGINT NOT NULL,
         message VARCHAR(250),
         received BOOLEAN NOT NULL
       );


